-- File: GateDefs.Mesa,  Last Edit: HGM  November 4, 1980  6:32 AMDIRECTORY  Process USING [Priority],  Window USING [Handle];GateDefs: DEFINITIONS =  BEGIN  StartupGateway, ShutdownGateway: PROCEDURE;  ScanParameterFile: PROCEDURE    RETURNS [priority: Process.Priority, noOiscp: BOOLEAN];  GetVersionText: PROCEDURE RETURNS [STRING];  GateDebugRecord: TYPE = RECORD [    version: STRING,    boot: POINTER TO POINTER, -- TO BootFileObject    cache: POINTER TO POINTER, -- TO CacheEntryObject    locks: POINTER TO POINTER, -- TO LockObject    packets, bytes: POINTER TO ARRAY [0..0) OF LONG CARDINAL,    nets: CARDINAL,    spare: POINTER];  typescript: Window.Handle;  END.