-- File: PupJuniperDefs.Mesa,  Last Edit: HGM  May 21, 1980  8:19 PMDIRECTORY  PupTypes USING [PupAddress, PupSocketID],  MachineIDDefs USING [MachineID];PupJuniperDefs: DEFINITIONS =  BEGIN  PupAddress: TYPE = PupTypes.PupAddress;  PupSocketID: TYPE = PupTypes.PupSocketID;  MachineIDFromPupAddress: PROCEDURE [PupAddress]    RETURNS [MachineIDDefs.MachineID];  PupAddressFromMachineID: PROCEDURE [MachineIDDefs.MachineID, PupSocketID]    RETURNS [PupAddress];  END.