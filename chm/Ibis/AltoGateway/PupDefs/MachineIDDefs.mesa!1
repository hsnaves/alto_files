-- File: MachineIDDefs.Mesa,  Last Edit: HGM  February 17, 1979  8:52 PMMachineIDDefs: DEFINITIONS =  BEGIN  -- COPIED FROM <Pilot>SystemInternal to avoid recompilation problems  GetMyMachineID: PROCEDURE RETURNS [MachineID];  MachineID, ProcessorID: TYPE = PRIVATE MACHINE DEPENDENT RECORD [    element: ElementType, series: CARDINAL];  SeriesType: TYPE = {altoFP, ethernet, ois};  ElementType: TYPE = RECORD [    SELECT COMPUTED SeriesType FROM      ethernet => [net: [0..256), host: [0..256)], -- series = ethernetSeries      ois => [CARDINAL], -- series IN OISProcessorSeries      ENDCASE];  OISProcessorSeries: TYPE = [0..177775B];  ethernetSeries: CARDINAL = 177776B;  altoFPSeries: CARDINAL = 177777B;  END.