// NewFileToolForAlto.cm, HGM, February 19, 1981  2:17 AM// Build a new FileTool after Changing PupStreamFTP Iris ^  Dir/c Mesa>System Retr/c ^    Segments.bcd Streams.bcd ^  Dir/c Tools>System Retr/c ^    Compatibility.bcd HeapString.bcd ToolDriver.bcd ^    Date.bcd Dates.bcd ^    STP.mesa STPOps.mesa STPReplyCode.bcd STPs.config ^    STPsA.mesa STPsB.mesa STPsC.mesa STPsD.mesa STPsE.mesa ^  Dir/c Tools>ToolSources Retr/c ^    FileToolOps.mesa FileToolDir.mesa FileSupport.mesa FileInterface.mesa FileTool.config// Alpha things (and mumble) that haven't been releasedFTP Igor ^  Dir/c AlphaTools>System Retr/c ^    STPOps.mesa STPsA.mesa STPsB.mesa STPsC.mesa STPsD.mesa ^//  Dir/c NewTools>Tools Retr/c ^//    FileSupport.mesa ^//  Dir/c PilotTools>Tools Retr/c ^//    FileInterface.mesa// HGM's fixes/copiesFTP Idun ^  Dir/c <Murray>Fixes>Tajo Retr/c ^    STPs.config STPsE.mesa FileTool.config FileSupport.mesa FileInterface.mesaCompile /a-m STP.mesa STPOps.mesa STPsA.mesa STPsB.mesa STPsC.mesa STPsD.mesa STPsE.mesa FileToolOps.mesa FileToolDir.mesa FileSupport.mesa FileInterface.mesaReal Binder STPs/cs FileTool/csDelete ^    Segments.bcd Streams.bcd ^    Date.bcd Dates.bcd ^    Compatibility.bcd HeapString.bcd ToolDriver.bcd ^    STP.mesa STPOps.mesa STPs.config ^    STPsA.mesa STPsB.mesa STPsC.mesa STPsD.mesa STPsE.mesa ^    STPOps.bcd STPReplyCode.bcd ^    STPsA.bcd STPsB.bcd STPsC.bcd STPsD.bcd STPsE.bcd ^    FileToolOps.bcd FileToolDir.bcd FileSupport.bcd FileInterface.bcd FileTool.config ^    FileToolOps.mesa FileToolDir.mesa FileSupport.mesa FileInterface.mesa