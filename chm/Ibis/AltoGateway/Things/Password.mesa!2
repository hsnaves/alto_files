-- File: Password.mesa,  Last Edit: HGM  March 18, 1981  9:29 PMPassword: DEFINITIONS =  BEGIN  -- Copied from Alto OS  Encrypted: TYPE [8];  Encrypt: PROCEDURE [name, password: STRING] RETURNS [Encrypted];  Check: PROCEDURE [password: STRING, old: Encrypted] RETURNS [BOOLEAN];  -- Talks to Grapevine  Status: TYPE = {yes, nil, allDown, notFound, badPwd, group, no, notGroup, error};  ValidMemberOfGroup: PROCEDURE [name, password, group: STRING] RETURNS [Status];  END.