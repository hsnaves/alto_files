// IfsVersion.bcpl - prints file system name and version// Copyright Xerox Corporation 1979, 1980, 1981, 1982, 1983// Last modified October 13, 1983  6:26 PM by Taftget "IFS.decl"external[// outgoing proceduresPrintIFSVersion// incoming proceduresPutTemplate// incoming staticsprimaryIFS; leafPresent]//----------------------------------------------------------------------------let PrintIFSVersion(stream, serverName) be//----------------------------------------------------------------------------[PutTemplate(stream, "$S 1.38$S", primaryIFS>>IFS.name, (leafPresent? "L", ""))if serverName ne 0 then   PutTemplate(stream, ", $S of October 13, 1983", serverName)]