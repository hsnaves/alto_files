/* FILE XCSTATES.H.    INCLUDED BY XCP.C *//* The following is a lookup table that is indexed by a TState name to return    a 16-bit actual Tvalue.  This is necessitated by there being 8-bit values    in EventTable and there being no variable-length structures in C *//* The keys */#define T0 00#define T1 01#define T2 02#define T3 03#define T11 04#define T16 05#define T24 06#define T28 07#define T50 08#define T51 09#define T56 010#define T60 011#define T61 012#define T66 013#define T68 014#define T70 015#define T78 016#define T83 017#define T93 018#define T95 019#define T114 020#define T119 021#define T128 022#define T132 023#define T134 023#define T136 025#define T143 026#define T145 027#define T146 028#define T164 029#define T168 030#define T178 031#define T193 032#define T194 033#define T195 034#define T197 035#define T198 036#define T209 037#define T210 038#define T219 039#define T254 040#define T256 041#define T258 042#define T262 043#define T267 044#define T271 045#define T275 046#define T302 047#define T314 048#define T315 049#define T316 050#define T340 051#define T351 052#define T353 053#define T358 054#define T385 055#define T430 056#define TLast 057int EvLookup[TLast]{0, 1, 2, 3, 11, 16, 24, 28, 50, 51, 56, 60, 61, 66, 68, 70, 78, 83, 93, 95,114, 119, 128, 132, 134, 136, 143, 145, 146, 164, 168, 178, 193, 194, 195, 197, 198,209, 210, 219, 254, 256, 258, 262, 267, 271, 275,302, 314, 315, 316, 340, 351, 353, 358, 385,430};