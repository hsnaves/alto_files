// IfsTelnetOEP.bcpl -- overlay entry point declarations for telnet server// Copyright Xerox Corporation 1979, 1981, 1982// Last modified April 9, 1982  4:40 PM by Taftexternal[// outgoing proceduresInitTelnetOEP// incoming proceduresDeclareOEP; TelnetGets; TelnetEndofs; TelnetResetInTelnetPuts; TelnetResetOut; TelnetTopLevel; TelnetErrorsTelnetOtherPup; TelnetCommandQuitExecLogin; ExecLogout; ExecConnect; ExecEnable; ExecDisableExecSystat; ExecStatistics; ExecDskStatExecList; ExecDelete; ExecRename; ExecDirectoryExecCreate; ExecDestroy; ExecShow; ExecChange; ExecHaltExecBackup; ExecInitialize; ExecExtend; ExecWhat; ExecDayTimeExecPress; ExecShowPress; ChangeDirSubcommand; SysParamSubcommand]//---------------------------------------------------------------------------let InitTelnetOEP(oev) be//---------------------------------------------------------------------------[DeclareOEP(oev, lv TelnetGets, lv TelnetEndofs, lv TelnetResetIn, lv TelnetPuts, lv TelnetResetOut)DeclareOEP(oev, lv TelnetTopLevel, lv TelnetErrors, lv TelnetOtherPup, lv TelnetCommandQuit)DeclareOEP(oev, lv ExecLogin, lv ExecLogout, lv ExecConnect, lv ExecEnable, lv ExecDisable, lv ExecChange, lv ExecDirectory)DeclareOEP(oev, lv ExecSystat, lv ExecStatistics, lv ExecDskStat, lv ExecList, lv ExecDelete, lv ExecCreate, lv ExecDestroy, lv ExecShow, lv ExecHalt)DeclareOEP(oev, lv ExecBackup, lv ExecInitialize, lv ExecExtend, lv ExecRename, lv ExecWhat, lv ExecDayTime, lv ExecPress, lv ExecShowPress, lv ChangeDirSubcommand, lv SysParamSubcommand)]