-- ImageDefsImpl.mesa-- Brenda Hankins 21-Jan-83 11:47:22-- this provides the alto proc StopMesa required by GVPServer.DIRECTORY  ImageDefs,  TemporaryBooting USING [BootFromVolume],  Volume USING [systemID];ImageDefsImpl: PROGRAM IMPORTS TemporaryBooting, Volume EXPORTS ImageDefs =  BEGIN  StopMesa: PUBLIC PROCEDURE =    BEGIN    TemporaryBooting.BootFromVolume[Volume.systemID, ];    -- switches default to DefaultBootSwitches set at installations time.    END;  END.  